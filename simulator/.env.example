# Mission Simulator Environment Variables
# Copy this file to .env and fill in your actual values

# Azure Function endpoint (where to send webhook messages)
AZURE_FUNCTION_ENDPOINT=https://respondrliteapp-d5614dea.azurewebsites.net

# Webhook API key (the 'code' parameter from your Azure Function URL)
WEBHOOK_API_KEY=your_webhook_api_key_here

# Preprod website endpoint (for keepalive pings)
PREPROD_WEB_ENDPOINT=https://preprod.scvsar.app

# Azure OpenAI configuration for GPT-5-nano message generation
AZURE_OPENAI_ENDPOINT=https://your-openai-endpoint.openai.azure.com/
AZURE_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Override the deployment name if different
# AZURE_OPENAI_DEPLOYMENT=gpt-5-nano

LOCAL_USER_NAME=your_local_username_here
LOCAL_USER_PASSWORD=your_local_password_here